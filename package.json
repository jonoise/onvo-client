{
  "name": "onvo-client",
  "version": "0.0.25",
  "description": "Onvo Client es un cliente de HTTP orientado a objetos para Node.js escrito en Typescript para interactuar de forma f√°cil con la API de Onvo.",
  "homepage": "https://github.com/jonoise/onvo-client",
  "bugs": {
    "url": "https://github.com/jonoise/onvo-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonoise/onvo-client.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.cjs",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "lint": "tsc",
    "pre": "yarn lint && yarn test",
    "build": "yarn pre && tsup src/index.ts --dts --minify --format cjs,esm --out-dir dist",
    "plocal": "rm -rf onvo-client-$npm_package_version.tgz && npm run build && npm pack"
  },
  "keywords": [
    "onvo",
    "onvopay",
    "pagos",
    "client",
    "api",
    "typescript",
    "nodejs"
  ],
  "funding": {
    "type": "github",
    "url": "https://www.paypal.com/paypalme/amilcarmunoz"
  },
  "author": "Amilcar Munoz",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {}
}
